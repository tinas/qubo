{
  "name": "qubo",
  "version": "0.1.0",
  "description": "MongoDB-like query builder for JavaScript/TypeScript objects with zero dependencies",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*.js",
    "dist/*.d.ts",
    "dist/operators/*.js",
    "dist/operators/*.d.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist coverage",
    "prebuild": "npm run clean",
    "build": "tsc && npm run build:esm",
    "build:esm": "tsc --module es2015 --outDir dist/esm",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint lib/**/*.ts",
    "lint:fix": "eslint lib/**/*.ts --fix",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm run lint",
    "version": "npm run lint:fix && git add -A lib",
    "postversion": "git push && git push --tags",
    "commitlint": "commitlint --edit",
    "prepare": "simple-git-hooks"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinas/qubo.git"
  },
  "keywords": [
    "query",
    "filter",
    "mongodb",
    "typescript",
    "javascript",
    "database",
    "search",
    "filter",
    "query-builder",
    "zero-dependencies"
  ],
  "author": {
    "name": "Ahmet Tinastepe",
    "email": "ahmttnstpe@gmail.com",
    "url": "https://github.com/tinas"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tinas/qubo/issues"
  },
  "homepage": "https://github.com/tinas/qubo#readme",
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@commitlint/types": "^19.5.0",
    "@jest/types": "^29.6.3",
    "@stylistic/eslint-plugin": "^1.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.16",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.9.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.5.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "simple-git-hooks": {
    "commit-msg": "npx --no -- commitlint --edit \"$1\""
  }
}